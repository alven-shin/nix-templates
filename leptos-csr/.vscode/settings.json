{
  "emmet.includeLanguages": {
    "rust": "html",
    "*.rs": "html"
  },
  "tailwindCSS.includeLanguages": {
    "rust": "html",
    "*.rs": "html"
  },
  "tailwindCSS.experimental.classRegex": [
    "\\.classes\\(\"([^\"]*)\"\\)",
    "\\.class\\(\"([^\"]*)\","
  ],
  "files.associations": {
    "*.rs": "rust"
  },
  "editor.quickSuggestions": {
    "other": "on",
    "comments": "on",
    "strings": true
  },
  "css.validate": false,
  "rust-analyzer.rustfmt.overrideCommand": ["rustyaura"]
}
